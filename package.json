{
    "productName": "Scorm Generator",
    "name": "scorm-generator",
    "version": "1.0.0",
    "homepage": "./",
    "author": "Webupps LLC",
    "description": "Webupps LLC own this product.",
    "repository": "https://github.com/webupps/scormgenerator",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
        "@fortawesome/free-regular-svg-icons": "^5.13.0",
        "@fortawesome/free-solid-svg-icons": "^5.13.0",
        "@fortawesome/react-fontawesome": "^0.1.9",
        "@popperjs/core": "^2.4.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "ace-builds": "^1.4.12",
        "aws-sdk": "^2.782.0",
        "axios": "^0.19.2",
        "chart.js": "^2.9.3",
        "child_process": "^1.0.2",
        "codemirror": "^5.54.0",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "electron-log": "^4.2.4",
        "electron-updater": "^4.3.5",
        "express": "^4.17.1",
        "formik": "^2.1.4",
        "jquery": "^3.5.1",
        "randomcolor": "^0.6.2",
        "react": "^16.13.1",
        "react-ace": "^9.1.3",
        "react-audio-player": "^0.13.0",
        "react-beautiful-dnd": "^13.0.0",
        "react-bootstrap": "^1.3.0",
        "react-chartjs-2": "^2.10.0",
        "react-codemirror2": "^7.2.0",
        "react-color": "^2.18.1",
        "react-contenteditable": "^3.3.4",
        "react-copy-to-clipboard": "^5.0.2",
        "react-csv-reader": "^3.1.1",
        "react-dom": "^16.13.1",
        "react-html-parser": "^2.0.2",
        "react-loadingg": "^1.7.2",
        "react-multi-select-component": "^3.0.1",
        "react-radio-group": "^3.0.3",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.1.2",
        "react-rte": "^0.16.3",
        "react-scripts": "^3.4.4",
        "react-toastify": "^6.0.8",
        "react-widgets": "^4.5.0",
        "redux": "^4.0.5",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "tree-kill": "^1.2.2",
        "typescript": "^3.8.3",
        "uuid": "^8.3.1",
        "video-react": "^0.14.1",
        "yup": "^0.28.5"
    },
    "main": "public/electron.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "start-electron-dev": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 && electron .",
        "start-electron-prod": "react-scripts build && cross-env NODE_ENV=production nodemon --exec electron .",
        "package-win": "react-scripts build && electron-builder build --win -c.extraMetadata.main=build/electron.js",
        "package-mac": "react-scripts build && electron-builder build --mac -c.extraMetadata.main=build/electron.js",
        "win-cert": "electron-builder create-self-signed-cert -p webupps",
        "publish-github": "electron-builder build --win -c.extraMetadata.main=build/electron.js -p onTagOrDraft",
        "publish-s3": "electron-builder build --win -c.extraMetadata.main=build/electron.js -p always"
    },
    "build": {
        "appId": "com.webupps.scormgenerator",
        "productName": "ScormGenerator",
        "copyright": "Copyright WebUpps LLC",
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "mac": {
            "category": "public.app-category.productivity",
            "target": "dmg",
            "extraFiles": [
                {
                    "from": "build/embedded/mac.jar",
                    "to": ""
                }
            ]
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ],
            "certificateFile": "private/webupps.pfx",
            "certificatePassword": "",
            "verifyUpdateCodeSignature": false,
            "requestedExecutionLevel": "highestAvailable",
            "publisherName": "webupps",
            "extraFiles": [
                {
                    "from": "build/embedded/windows.jar",
                    "to": ""
                }
            ]
        },
        "linux": {
            "category": "Utility"
        },
        "dmg": {
            "artifactName": "Setup_${version}.${ext}"
        },
        "nsis": {
            "shortcutName": "Scorm Generator",
            "artifactName": "Setup_${version}.${ext}",
            "oneClick": false,
            "perMachine": true,
            "allowToChangeInstallationDirectory": true
        },
        "publish": {
            "provider": "s3",
            "bucket": "scorm-generator-dev",
            "region": "ap-southeast-1",
            "path": "/release"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "electron": "^10.1.3",
        "electron-builder": "^22.8.1",
        "nodemon": "^2.0.0"
    }
}
